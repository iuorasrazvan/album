{
    "name": "iuorasrazvan/register-login-album-management",
	
    "description": "A basic application for user and album management",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "stable",
    "prefer-stable": true,

	
	
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-component-installer": "^1.0 || ^0.7 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.0.1",
        "zfcampus/zf-development-mode": "^3.0",
        "zendframework/zend-cache": "^2.7.1",
        "zendframework/zend-db": "^2.8.1",
        "zendframework/zend-mvc-form": "^1.0",
        "zendframework/zend-log": "^2.9",
        "zendframework/zend-mvc-console": "^1.1",
        "zendframework/zend-session": "^2.8",
        "zendframework/zend-servicemanager-di": "^1.0",
        "doctrine/common": "^2.8",
        "zendframework/zend-captcha": "^2.7",
        "zendframework/zend-mvc-plugin-fileprg": "^1.1",
        "zendframework/zend-hydrator": "^2.4",
        "zendframework/zend-authentication": "^2.6",
        "zendframework/zend-crypt": "^3.3",
        "zendframework/zend-config": "^3.2",
        "zendframework/zend-permissions-acl": "^2.7",
        "zendframework/zend-permissions-rbac": "^3.0",
        "zendframework/zend-paginator": "^2.8",
        "zendframework/zend-navigation": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/",
			"Album\\":"module/Album/src/", 
			"Login\\":"module/Login/src/", 
			"Database\\":"module/Database/src/",
			"Driver\\":"module/Driver/src/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit"
    },
    "require-dev": {
        "zendframework/zend-test": "^3.0.1"
    }
}
